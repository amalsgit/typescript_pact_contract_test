{
  "consumer": {
    "name": "Movie Consumer"
  },
  "provider": {
    "name": "Movie Producer"
  },
  "interactions": [
    {
      "description": "a request for returning all movies",
      "providerState": "I have a list of movies",
      "request": {
        "method": "GET",
        "path": "/movies"
      },
      "response": {
        "status": 200,
        "headers": {
        },
        "body": [
          {
            "id": 1,
            "name": "foo bar",
            "year": 1989
          }
        ],
        "matchingRules": {
          "$.body": {
            "min": 1
          },
          "$.body[*].*": {
            "match": "type"
          },
          "$.body[*].id": {
            "match": "type"
          },
          "$.body[*].name": {
            "match": "type"
          },
          "$.body[*].year": {
            "match": "type"
          }
        }
      }
    }
  ],
  "metadata": {
    "pactSpecification": {
      "version": "2.0.0"
    }
  }
}